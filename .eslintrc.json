{
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jasmine/recommended"
    ],
    "installedESLint": true,
    "plugins": [
      "react",
      "sorting",
      "jasmine"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "jasmine": true
    },
    "rules": {
      "strict": 0,
      "global-require": "off",
      "no-use-before-define": "off",
      "sort-vars": "error",
      "sort-imports": "off",
      "sorting/sort-object-props": ["error", { "ignoreCase": true, "ignoreMethods": false } ],
      "import/order": ["error", { "groups": ["builtin", "external", "internal","parent", "sibling", "index"] }],
      "react/jsx-no-bind": "off",
      "object-curly-spacing": "off"
    }
}
